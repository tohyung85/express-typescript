{
  "name": "josh-app",
  "version": "1.0.0",
  "description": "my app",
  "main": "server.ts",
  "scripts": {
    "build": "tsc",
    "dev": "./node_modules/nodemon/bin/nodemon.js",
    "start": "NODE_ENV=default ts-node ./src/server.ts",
    "prod": "npm run build && ts-node ./dist/server.js",
    "test": "NODE_ENV=test mocha -r ts-node/register src/**/**/tests/*.test.ts --exit",
    "test-server": "NODE_ENV=test ts-node ./src/server.ts",
    "migrate-test": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -c test",
    "migrate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run",
    "rollback": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^10.12.12",
    "chai-http": "^4.2.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^3.0.5",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.118",
    "@types/mocha": "^5.2.5",
    "@types/passport": "^0.4.7",
    "@types/passport-jwt": "^3.0.1",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1",
    "mysql": "^2.16.0",
    "nodemon": "^1.18.7",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.2.9"
  }
}
